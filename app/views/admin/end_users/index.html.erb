<h1>ユーザ一覧</h1>

<table class="table" border="1" style="border-collapse: collapse">
  <thead>
    <th>id</th>
    <th>ニックネーム</th>
    <th>メールアドレス</th>
    <th>投稿数</th>
  </thead>
  <tbody>
    <% @end_users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td>
          <%= link_to admin_end_user_path(user.id) do %>
            <%= user.name %>
          <% end %>
        </td>
        <td><%= user.email %></td>
        <td></td>
      </tr>
    <% end %>
  </tbody>
</table>
